group nbu_SG (
	SystemList = { sbpnd01ba144 = 0 }
        ClusterList = { netbackup_sbpnd01wm144 = 0, netbackup_sbpnd01sp144 = 1, netbackup_sbpnd01ba144 = 2 }
	AutoStart = 0
	Frozen = 1
	AutoFailOver = 0
	AutoStartList = { sbpnd01ba144 }
	TriggersEnabled @sbpnd01ba144 = { POSTONLINE, POSTOFFLINE }
	)

	DiskGroup nbudg_DG (
		DiskGroup = nbudg
		)

	IPMultiNICB nbu_IPMNICB (
		BaseResName = nbu_MNICB
		Address @sbpnd01ba144 = "172.26.100.67"
		NetMask @sbpnd01ba144 = "255.255.254.0"
		)

	Mount netbackup_MNT (
		FsckOpt = "-y"
		BlockDevice = "/dev/vx/dsk/nbudg/nbuvol"
		MountPoint = "/netbackup"
		FSType = vxfs
		)

	MultiNICB nbu_MNICB (
		Device = { igb0 = 0, igb4 = 1 }
		UseMpathd = 1
		)

	NFS NFS_server (
		Critical = 0
		)

	NetBackup server_NBU (
		AutoStart = 0
		Critical = 0
		ServerName = NBU_Server
		ServerType = NBUMaster
		RSPFile = "/usr/openv/netbackup/bin/cluster/NBU_RSP"
		GroupName = nbu_SG
		MonScript = NONE
		)

	Share catalogBU_share (
		Critical = 0
		Options = "ro,anon=0"
		PathName = "/netbackup/catalogBU/share"
		)

	Volume nbuvol_VOL (
		Volume = nbuvol
		DiskGroup = nbudg
		)

	catalogBU_share requires NFS_server
	catalogBU_share requires netbackup_MNT
	nbu_IPMNICB requires nbu_MNICB
	nbuvol_VOL requires nbudg_DG
	netbackup_MNT requires nbuvol_VOL
	server_NBU requires nbu_IPMNICB
	server_NBU requires netbackup_MNT


