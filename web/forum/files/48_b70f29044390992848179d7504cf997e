group catalogBU_SG (
	SystemList = { sbpnd01ba144 = 0 }
        ClusterList = { netbackup_sbpnd01wm144 = 0, netbackup_sbpnd01sp144 = 1, netbackup_sbpnd01ba144 = 2 }
	Frozen = 1
	AutoStart = 0
	AutoFailOver = 0
	AutoStartList = { sbpnd01ba144 }
	TriggersEnabled @sbpnd01ba144 = { POSTONLINE, POSTOFFLINE }
	)

	DiskGroup catrepdg_DG (
		DiskGroup = catrepdg
		)

	Mount catrep_MNT (
		FsckOpt = "-y"
		BlockDevice = "/dev/vx/dsk/catrepdg/catrepnvol"
		MountPoint = "/repository"
		FSType = vxfs
		)

	Volume catrepvol_VOL (
		Volume = catrepnvol
		DiskGroup = catrepdg
		)

	catrep_MNT requires catrepvol_VOL
	catrepvol_VOL requires catrepdg_DG


