system sbpnd01ba144 (
	)

group ClusterService (
	SystemList = { sbpnd01ba144 = 0 }
	AutoStartList = { sbpnd01ba144 }
	TriggersEnabled @sbpnd01ba144 = { POSTONLINE, POSTOFFLINE }
	OnlineRetryLimit = 3
	OnlineRetryInterval = 120
	)

	
        Application wac (
                StartProgram = "/opt/VRTSvcs/bin/wacstart"
                MonitorProcesses = { "/opt/VRTSvcs/bin/wac" }
                StopProgram = "/opt/VRTSvcs/bin/wacstop"
                RestartLimit = 3
                )

        IPMultiNICB gcoip (                                   
                BaseResName = nbu_MNICB
                Address @sbpnd01ba144 = "172.26.100.68"
                NetMask @sbpnd01ba144 = "255.255.254.0"
                )

	NotifierMngr ntfr (
		SmtpFromPath = "unix.admin@scotiabank.com"
		SmtpServer = "appmailrelay.gss.scotia-capital.com"
		SmtpRecipients = { "unix.admin@scotiabank.com" = Warning }
		)

	Proxy nbu_multinic_PROXY (
		TargetResName = nbu_MNICB
		)

	ntfr requires nbu_multinic_PROXY
        gcoip requires nbu_multinic_PROXY
        wac requires gcoip



